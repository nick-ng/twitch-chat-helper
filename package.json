{
  "name": "twitch-chat-helper",
  "version": "1.0.0",
  "description": "Alert you when someone chats",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build-only": "cross-env NODE_ENV=production webpack",
    "build": "cross-env NODE_ENV=production webpack && cp ./dist/index.html ./dist/404.html && cp ./static-html/* ./dist/* && echo \"twitch-chat.pux.one\" > ./dist/CNAME",
    "deploy": "gh-pages -d dist -r https://${GH_TOKEN}@github.com/${GITHUB_REPOSITORY}.git",
    "dev": "cross-env NODE_ENV=development webpack serve",
    "pretty": "pretty-quick --staged",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nick-ng/twitch-chat-helper.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nick-ng/twitch-chat-helper/issues"
  },
  "homepage": "https://github.com/nick-ng/twitch-chat-helper#readme",
  "devDependencies": {
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "webpack-dev-server": "^3.11.1"
  },
  "dependencies": {
    "@babel/compat-data": "^7.12.7",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@loadable/component": "^5.14.1",
    "babel-loader": "^8.2.2",
    "case": "^1.6.3",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "dayjs": "^1.11.1",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.5.1",
    "lodash": "^4.17.21",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "style-loader": "^2.0.0",
    "styled-components": "^5.2.1",
    "tmi.js": "^1.8.5",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1"
